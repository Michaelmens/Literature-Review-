@article{ WOS:001191316600001,
Author = {Young-Ferris, Anna and Malik, Arunima and Calderbank, Victoria and
   Jacob-John, Jubin},
Title = {Making things (that don't exist) count: a study of Scope 4 emissions
   accounting claims},
