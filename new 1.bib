@article{ WOS:000457448300012,
Author = {Bushi, Lindita and Skszek, Tim and Reaburn, Timothy},
Title = {New ultralight automotive door life cycle assessment},
Journal = {INTERNATIONAL JOURNAL OF LIFE CYCLE ASSESSMENT},
