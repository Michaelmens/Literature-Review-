@article{ WOS:001200136600001,
Author = {Yuan, Xuehong and Wang, Jianbo and Song, Qingming and Xu, Zhenming},
Title = {Integrated assessment of economic benefits and environmental impact in
   waste glass closed-loop recycling for promoting glass circularity},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2024},
Volume = {444},
Month = {MAR 10},
Abstract = {Facing abundant waste glass generation globally, closed-loop recycling
   is promising by substituting raw materials with recovered secondary
   materials. Adequate economic-environmental information is crucial to
   promote glass recycling. However, previous economic-environmental
   analysis predominantly concentrated on open-loop recycling; and
   distinctions among different glass categories were neglected. Hence,
   this study explored economicenvironmental benefits of closed-loop
   recycling for different kinds of waste glass with techno-economic
   analysis and life-cycle assessment, covering transportation, crushing \&
   separation, and waste utilization stages. Firstly, recycling status
   investigation identified waste flat glass (WFG) and waste packaging
   glass (WPG) as research objects, collectively representing over 90\% of
   glass. Based on data inventory established through material and energy
   flow analysis, our further assessment reveals 11.6 USD/t economic
   benefits for WFG recycling and -6.9 to -0.6 USD/t for WPG; total
   environmental impact after standardization is -51\% and -29\% for WFG
   and WPG closed-loop recycling, respectively. Finally, scenario analysis
   highlights the importance of enhancing the separation efficiency of the
   crushing \& separation stage and improving the ratio of substituting raw
   materials with waste in the waste utilization stage, which are the most
   efficient ways to increase economic and environmental benefits,
   respectively. Our work provides critical insights for targeted
   development of waste glass recycling industry.},
