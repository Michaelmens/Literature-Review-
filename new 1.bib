@article{ WOS:000700117900005,
Author = {Cooney, Ronan and Wan, Alex H. L. and O'Donncha, Fearghal and Clifford,
   Eoghan},
Title = {Designing environmentally efficient aquafeeds through the use of
   multicriteria decision support tools},
Journal = {CURRENT OPINION IN ENVIRONMENTAL SCIENCE \& HEALTH},
Year = {2021},
Volume = {23},
Month = {OCT},
Abstract = {Aquaculture is the fastest growing food production system, and the
   sector accounts for more than half of all fish consumed. Its potential
   as a sustainable food source has been recognised within the EU Farm to
   Fork strategy and by the targeting of EU Green Deal research funds.
   Aquafeed is the primary source of environmental cost in farmed finfish
   and shrimp life cycle assessments (LCA), and thus, emerging ingredients
   have a key role to play in increasing its sustainability. This paper
   proposes that the development and `design' of feed compositions can be
   disrupted by innovative approaches that simultaneously optimise-via
   multicriteria analysis-ingredient selection through leveraging economic,
   nutritional, life cycle cost and environmental datasets. This would be
   supported by the ongoing collection and curation of these same
   resources. Further, integration of these disparate resources can enable
   advanced ingredient traceability. The outcome of such an approach would
   greatly contribute to increasingly efficient aquafeed production. (C)
   2021 The Author(s). Published by Elsevier B.V.},
Publisher = {ELSEVIER},
Address = {RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS},
Type = {Article},
Language = {English},
Affiliation = {Cooney, R (Corresponding Author), Natl Univ Ireland Galway, Sch Engn, Galway H91 HX31, Ireland.
   Cooney, R (Corresponding Author), Natl Univ Ireland Galway, Ryan Inst, Galway H91 TK33, Ireland.
