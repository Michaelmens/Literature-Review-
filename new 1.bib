@article{ WOS:000413579000026,
Author = {Landeta-Manzano, Benat and Arana-Landin, German and RuizdeArbulo, Patxi
   and DiazdeBasurto, Pablo},
Title = {Longitudinal Analysis of the Eco-Design Management Standardization
   Process in Furniture Companies},
Journal = {JOURNAL OF INDUSTRIAL ECOLOGY},
