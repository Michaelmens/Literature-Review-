@article{ WOS:000429025000017,
Author = {Lenzo, Paola and Traverso, Marzia and Mondello, Giovanni and Salomone,
   Roberta and Ioppolo, Giuseppe},
Title = {Sustainability Performance of an Italian Textile Product},
