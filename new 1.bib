@article{ WOS:000649673000004,
Author = {Jang, Hayoung and Jeong, Byongug and Zhou, Peilin and Ha, Seungman and
   Nam, Dong},
Title = {Demystifying the lifecycle environmental benefits and harms of LNG as
   marine fuel},
