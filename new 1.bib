@article{ WOS:000427338900052,
Author = {Li, Yi and Huang, Youyi and Ye, Quanliang and Zhang, Wenlong and Meng,
   Fangang and Zhang, Shanxue},
Title = {Multi-objective optimization integrated with life cycle assessment for
   rainwater harvesting systems},
Journal = {JOURNAL OF HYDROLOGY},
Year = {2018},
Volume = {558},
Pages = {659-666},
Month = {MAR},
Abstract = {The major limitation of optimization models applied previously for
   rainwater harvesting (RWH) systems is the systematic evaluation of
   environmental and human health impacts across all the lifecycle stages.
   This study integrated life cycle assessment (LCA) into a multi-objective
   optimization model to optimize the construction areas of green rooftops,
   porous pavements and green lands in Beijing of China, considering the
   trade-offs among 24 h-interval RWH volume (Q(R)), stormwater runoff
   volume control ratio (R), economic cost (E-C), and environmental impacts
   (EI). Eleven life cycle impact indicators were assessed with a
   functional unit of 10,000 m(2) of RWH construction areas. The LCA
   results showed that green lands performed the smallest lifecycle impacts
