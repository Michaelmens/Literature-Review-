@article{ WOS:000391079300099,
Author = {Kuczenski, Brandon and Davis, Christopher B. and Rivela, Beatriz and
   Janowicz, Krzysztof},
Title = {Semantic catalogs for life cycle assessment data},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2016},
Volume = {137},
