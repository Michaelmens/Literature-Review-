@article{ WOS:000413579000006,
Author = {Bukoski, Jacob J. and Chaiwiwatworakul, Pipat and Gheewala, Shabbir H.},
Title = {The Life Cycle Assessment of an Energy-Positive Peri-Urban Residence in
   a Tropical Regime},
Journal = {JOURNAL OF INDUSTRIAL ECOLOGY},
